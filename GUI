local DiscordLib = loadstring(game:HttpGet"https://pastebin.com/raw/za26qWBJ")()
local win = DiscordLib:Window("discord library")
local serv = win:Server("Hungry Pig GUI", "")

local btns = serv:Channel("Server")
btns:Toggle("Auto Grab Food", false, function(e)
	if e then
		loadstring(game:HttpGet('https://raw.githubusercontent.com/Pixelated03/Hungry-Pig/main/Auto%20Grab%20Food'))()
	else
		getgenv().AutoBearTrapPigs=true
	end
end)
btns:Seperator()
btns:Button("Inf Coins", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/Pixelated03/Hungry-Pig/main/Inf%20Coins'))()
end)
btns:Seperator()
btns:Toggle("Spam Bear Trap", false, function(e)
	if e then
		loadstring(game:HttpGet('https://raw.githubusercontent.com/Pixelated03/Hungry-Pig/main/Spam%20Bear%20Trap'))()
	else
		getgenv().AutoBearTrapPigs=true
	end
end)
btns:Toggle("Auto Gun Shot Pigs", false, function(e)
	if e then
		loadstring(game:HttpGet('https://raw.githubusercontent.com/Pixelated03/Hungry-Pig/main/Auto%20Gun%20Shot%20Pigs'))()
	else
		getgenv().AutoBearTrapPigs=true
	end
end)

local btns = serv:Channel("Player")
btns:Slider("WalkSpeed", 20, 40, 20, function(g)
	local h=game.Players.LocalPlayer.Character
	if h then
		local i=h:FindFirstChildOfClass("Humanoid")
		if i then
			i.WalkSpeed=g
		end
	end
end)
btns:Seperator()
btns:Toggle("Name Hidder", false, function(f)
	if f then
		loadstring(game:HttpGet('https://raw.githubusercontent.com/Pixelated03/Name-Hidder/main/Initializer'))()
	else
		NameHidder()
	end
end)
btns:Seperator()
btns:Button("Infinite Yield", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)
